{
  "arch": [
    "amd64",
    "aarch64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "description": "TriliumNext Notes is a hierarchical note taking application with focus on building large personal knowledge bases.",
  "environment": {
    "TRILIUM_DATA_DIR": "/share/trilium",
    "NO_AUTHENTICATION": "NO_AUTHENTICATION",
    "TRILIUM_OAUTH_BASE_URL": "TRILIUM_OAUTH_BASE_URL",
    "TRILIUM_OAUTH_CLIENT_ID": "TRILIUM_OAUTH_CLIENT_ID",
    "TRILIUM_OAUTH_CLIENT_SECRET": "TRILIUM_OAUTH_CLIENT_SECRET",
    "TRILIUM_OAUTH_ISSUER_BASE_URL": "TRILIUM_OAUTH_ISSUER_BASE_URL",
    "TRILIUM_OAUTH_ISSUER_NAME": "TRILIUM_OAUTH_ISSUER_NAME"
  },
  "name": "Trilium Next - JB",
  "map": [
    "share:rw"
  ],
  "options": {
    "NO_AUTHENTICATION": true,
    "TRILIUM_OAUTH_BASE_URL": "",
    "TRILIUM_OAUTH_CLIENT_ID": "",
    "TRILIUM_OAUTH_CLIENT_SECRET": "",
    "TRILIUM_OAUTH_ISSUER_BASE_URL": "",
    "TRILIUM_OAUTH_ISSUER_NAME": ""
  },
  "schema": {
    "NO_AUTHENTICATION": "bool",
    "TRILIUM_OAUTH_BASE_URL": "str",
    "TRILIUM_OAUTH_CLIENT_ID": "str",
    "TRILIUM_OAUTH_CLIENT_SECRET": "str",
    "TRILIUM_OAUTH_ISSUER_BASE_URL": "str",
    "TRILIUM_OAUTH_ISSUER_NAME": "str"
  },
  "expose": {
    "8080/tcp": {}
  },
  "ports_description": {
    "8080/tcp": "Web interface"
  },
  "slug": "triliumnext",
  "startup": "services",
  "upstream": "v0.95.0",
  "url": "https://github.com/jeakob/homeassistant-addons",
  "version": "v0.95.0",
  "ingress": true,
  "ingress_port": 8080,
  "image": "triliumnext/notes",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8080]",
  "tmpfs": "size=100m,uid=1000,gid=1000"
}
